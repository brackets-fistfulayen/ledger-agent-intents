{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build:styles": {
			"outputs": ["dist/styles.css"]
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".output/**", ".vinxi/**"]
		},
		"dev": {
			"dependsOn": ["^build:styles"],
			"cache": false,
			"persistent": true
		},
		"typecheck": {
			"dependsOn": ["^build"]
		},
		"clean": {
			"cache": false
		}
	}
}
